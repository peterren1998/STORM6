<!-- speed 200 ~= 833 uL / 10 sec -->
<!-- 1 uL corresponds to 1.2 pump position units -->
<!-- speed 33 corresponds to 1 mL / min -->
  <pump_commands>
     <pump_cmd name = "Flow 0.2mL">
       <pump_config position = "240" speed = "50"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Flow 0.3mL">
       <pump_config position = "360" speed = "50"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Flow 0.5mL">
       <pump_config position = "600" speed = "50"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Flow 1mL">
       <pump_config position = "1200" speed = "50"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Flow 2mL">
       <pump_config position = "2400" speed = "50"></pump_config>
    </pump_cmd>
    <pump_cmd name = "Flow 3mL">
       <pump_config position = "3600" speed = "50"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Flow 0.5mL fast">
       <pump_config position = "600" speed = "100"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Flow 1mL fast">
       <pump_config position = "1200" speed = "100"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Flow 1.5mL fast">
       <pump_config position = "1800" speed = "100"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Flow 2mL fast">
       <pump_config position = "2400" speed = "100"></pump_config>
     </pump_cmd>

    <pump_cmd name = "Stop Flow">
      <pump_config speed = "0.0"></pump_config>
    </pump_cmd>

  </pump_commands>

  <kilroy_protocols>
    <protocol name = "Incubate 10min">
	    <pump duration = "600">Stop Flow</pump>
    </protocol>

    <protocol name = "Hyb primary antibody">
     	<valve duration = "5">Set Hyb 18</valve>
    	<pump duration = "150">Flow 2mL</pump>
     	<valve duration = "5">Set Other</valve>
      <!-- because of long tubeing.. push some wash buffer to push more TCEP into chamber -->
      <pump duration = "60">Flow 0.5mL</pump>
      <valve duration = "3600">Set Other</valve> 
    	<pump duration = "300">Flow 2mL</pump>
      <pump duration = "300">Flow 2mL</pump>
      <pump duration = "300">Flow 2mL</pump>
      <valve duration = "5">Set Other</valve>
    </protocol>


  	<protocol name = "Cleave">
     	<valve duration = "5">Set TCEP</valve>
    	<pump duration = "150">Flow 2mL</pump>
     	<valve duration = "5">Set Wash</valve>
      <!-- because of long tubeing.. push some wash buffer to push more TCEP into chamber -->
      <pump duration = "500">Flow 0.5mL</pump> 
    	<pump duration = "180">Flow 2mL</pump>
      <pump duration = "60">Flow 0.5mL</pump> 
    </protocol>

    <protocol name = "Flow Image Buffer">
        <valve duration = "5">Set Image</valve>
        <pump duration = "150">Flow 2mL</pump>
        <pump duration = "50">Flow 0.5mL</pump>
        <valve duration = "5">Set Other</valve>
        <!-- because of long tubeing.. push some PBS to push more image buffer into chamber -->
        <pump duration = "100">Flow 0.5mL</pump>
        <valve duration = "5">Set Image</valve> 
    </protocol>    
    
    <protocol name = "Flow Readout">
      <valve duration = "5">Set Readout</valve>
      <pump duration = "150">Flow 2mL</pump>
     	<valve duration = "5">Set Wash</valve>
      <!-- because of long tubeing.. push some wash buffer to push more TCEP into chamber -->
      <pump duration = "500">Flow 0.5mL</pump> 
    	<pump duration = "180">Flow 2mL</pump>
      <pump duration = "60">Flow 0.5mL</pump> 
    </protocol> 

    <protocol name = "Flow Wash">
      <valve duration = "5">Set Wash</valve>
    	<pump duration = "180">Flow 2mL</pump>
      <pump duration = "60">Flow 0.5mL</pump>
    </protocol>    

     <protocol name = "Flow Other">
        <valve duration = "5">Set Other</valve>
        <pump duration = "300">Flow 2mL</pump>
    </protocol>  

